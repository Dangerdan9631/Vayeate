{
    "name": "vayeate",
    "displayName": "Vayeate",
    "icon": "./images/icon.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/Dangerdan9631/Vayeate"
    },
    "description": "A custom color theme for VS Code",
    "publisher": "Dangerdan9631",
    "version": "1.2.0",
    "engines": {
        "vscode": "^1.76.0"
    },
    "categories": [
        "Themes"
    ],
    "contributes": {
        "themes": [
            {
                "label": "Vayeate",
                "uiTheme": "vs-dark",
                "path": "./themes/vayeate-color-theme.json"
            },
            {
                "label": "Vayeate Light",
                "uiTheme": "vs",
                "path": "./themes/vayeate-light-color-theme.json"
            },
            {
                "label": "Mercurius",
                "uiTheme": "vs-dark",
                "path": "./themes/mercurius-color-theme.json"
            }
        ]
    },
    "scripts": {
        "install-extension": "vsce package && code --install-extension vayeate-1.1.0.vsix"
    }
}