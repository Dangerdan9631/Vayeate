{
    "name": "Mercurius",
    "type": "dark",
    "semanticHighlighting": true,
    "colors": {
        // =====================================================================
        // Title Bar
        "titleBar.activeBackground": "#993333",
        "titleBar.activeForeground": "#ff9999",
        "titleBar.border": "#993333",
        "titleBar.inactiveBackground": "#7e5f5f",
        "titleBar.inactiveForeground": "#cca2a2",
        "focusBorder": "#993333",
        // =====================================================================
        // Menu Bar
        "menu.background": "#993333",
        "menu.foreground": "#ff9999",
        "menu.separatorBackground": "#ff9999",
        "menu.selectionForeground": "#ffcce8",
        "menu.selectionBackground": "#cc6666",
        "menubar.selectionForeground": "#ffcce8",
        "menubar.selectionBackground": "#cc6666",
        // =====================================================================
        // Activity Bar
        "activityBar.foreground": "#cc3333",
        "activityBar.background": "#5f1b1b",
        "activityBar.activeBorder": "#cc3333",
        "activityBar.dropBorder": "#ffcce8",
        "activityBar.inactiveForeground": "#993333",
        // =====================================================================
        // Breadcrumb
        "breadcrumb.foreground": "#cc3333",
        "breadcrumb.background": "#5f1b1b",
        "breadcrumb.focusForeground": "#ff9999",
        "breadcrumb.activeSelectionForeground": "#cc3333",
        "breadcrumbPicker.background": "#5f1b1b",
        // =====================================================================
        // Side Bar
        "sideBar.background": "#3e2e2e",
        "sideBar.foreground": "#ccbbbb",
        "sideBarSectionHeader.background": "#5f1b1b",
        "sideBarSectionHeader.foreground": "#cc3333",
        "sideBarTitle.foreground": "#ccbbbb",
        "sideBar.dropBackground": "#993333",
        "sideBar.border": "#5f1b1b",
        // =====================================================================
        // Status Bar
        "statusBar.background": "#993333",
        "statusBar.foreground": "#ff9999",
        "statusBar.debuggingBackground": "#3b6e4c",
        "statusBar.debuggingForeground": "#ccbbbb",
        "statusBarItem.remoteForeground": "#ff0000",
        "statusBar.noFolderBackground": "#a79560",
        "statusBar.noFolderForeground": "#ccbbbb",
        "statusBarItem.activeBackground": "#ccbbbb2e",
        "statusBarItem.errorBackground": "#c72e0f",
        "statusBarItem.errorForeground": "#ffffff",
        "statusBarItem.hoverBackground": "#ffffff2e",
        "progressBar.background": "#ff9999",
        // =====================================================================
        // Notifications
        "notifications.foreground": "#ff9999",
        "notifications.background": "#5f1b1b",
        "notifications.border": "#5f1b1b",
        "notificationToast.border": "#5f1b1b",
        "notificationCenterHeader.foreground": "#ff9999",
        "notificationCenterHeader.background": "#993333",
        "notificationCenter.border": "#993333",
        "notificationsInfoIcon.foreground": "#ff9999",
        "notificationsWarningIcon.foreground": "#cc9900",
        "notificationsErrorIcon.foreground": "#996633",
        "notificationLink.foreground": "#cc3333",
        // =====================================================================
        // Extension Pane
        "extensionButton.prominentBackground": "#993333",
        "extensionButton.prominentForeground": "#ff9999",
        "extensionButton.prominentHoverBackground": "#803434",
        // =====================================================================
        // Editor View
        "editor.foreground": "#b69f9f",
        "editor.background": "#352828",
        "editorCursor.foreground": "#cc9900",
        "editor.lineHighlightBackground": "#3f2f2f",
        "editor.selectionBackground": "#6d3434",
        "editorLineNumber.foreground": "#cc6666",
        "editorLineNumber.activeForeground": "#cc6666",
        "editorIndentGuide.background1": "#6d3434",
        "editorIndentGuide.background2": "#6d3434",
        "editorIndentGuide.background3": "#6d3434",
        "editorIndentGuide.background4": "#6d3434",
        "editorIndentGuide.background5": "#6d3434",
        "editorIndentGuide.background6": "#6d3434",
        "editorIndentGuide.activeBackground1": "#cc6666",
        "editorIndentGuide.activeBackground2": "#cc6666",
        "editorIndentGuide.activeBackground3": "#cc6666",
        "editorIndentGuide.activeBackground4": "#cc6666",
        "editorIndentGuide.activeBackground5": "#cc6666",
        "editorIndentGuide.activeBackground6": "#cc6666",
        "editorInlayHint.foreground": "#8f7070",
        "editorInlayHint.background": "#463b3b",
        // =====================================================================
        // Gutter
        "editorGutter.addedBackground": "#6fad6e",
        "editorGutter.background": "#461b1b",
        "editorGutter.deletedBackground": "#fc7676",
        "editorGutter.modifiedBackground": "#cc333366",
        // =====================================================================
        // Editor matching
        "editor.findMatchBackground": "#176431",
        "editor.findMatchHighlightBackground": "#174e64",
        "editor.findRangeHighlightBackground": "#cc333311",
        "editorLink.activeForeground": "#ce4e4e",
        "editorCodeLens.foreground": "#cc333366",
        "editorBracketMatch.background": "#701717",
        "editorBracketMatch.border": "#cc333366",
        "editorRuler.foreground": "#573333",
        // =====================================================================
        // Editor Group
        "editorGroupHeader.tabsBackground": "#352828",
        "editorGroup.emptyBackground": "#352828",
        // =====================================================================
        // Git and Diff Editor
        "gitDecoration.conflictingResourceForeground": "#cc9900",
        "gitDecoration.deletedResourceForeground": "#b90e0e",
        "gitDecoration.ignoredResourceForeground": "#a35e5e",
        "gitDecoration.modifiedResourceForeground": "#618394",
        "gitDecoration.untrackedResourceForeground": "#999966",
        "diffEditor.insertedTextBackground": "#07792733",
        "diffEditor.removedTextBackground": "#793d3d33",
        // =====================================================================
        // Terminal Colors
        "terminal.ansiBlack": "#000000",
        "terminal.ansiBlue": "#b86161",
        "terminal.ansiBrightBlack": "#90a4ae",
        "terminal.ansiBrightBlue": "#b86161",
        "terminal.ansiBrightCyan": "#39adb5",
        "terminal.ansiBrightGreen": "#91b859",
        "terminal.ansiBrightMagenta": "#7c4dff",
        "terminal.ansiBrightRed": "#e53935",
        "terminal.ansiBrightWhite": "#ffffff",
        "terminal.ansiBrightYellow": "#ffb62c",
        "terminal.ansiCyan": "#39adb5",
        "terminal.ansiGreen": "#91b859",
        "terminal.ansiMagenta": "#7c4dff",
        "terminal.ansiRed": "#e53935",
        "terminal.ansiWhite": "#ffffff",
        "terminal.ansiYellow": "#ffb62c",
        "terminal.border": "#4f384a",
        "terminal.foreground": "#f6f6f4",
        "terminal.background": "#352828",
        "textPreformat.foreground": "#b69f9f",
        // =====================================================================
        // Widgets
        "editorWidget.foreground": "#ff9999",
        "editorWidget.background": "#5f1b1b",
        "editorWidget.border": "#5f1b1b",
        "widget.shadow": "#15181b",
        // =====================================================================
        // Badges
        "activityBarBadge.foreground": "#ff9999",
        "activityBarBadge.background": "#cc3333",
        "badge.foreground": "#ff9999",
        "badge.background": "#cc3333",
        "extensionBadge.remoteForeground": "#ff9999",
        "extensionBadge.remoteBackground": "#cc3333",
        // =====================================================================
        // Panels
        "panel.border": "#993333",
        "panelTitle.activeForeground": "#ff9999",
        "panelTitle.activeBorder": "#ff9999",
        "panelTitle.inactiveForeground": "#cca2a2",
        "panel.background": "#5f1b1b",
        "panel.dropBorder": "#993333",
        "panelSection.border": "#993333",
        "panelSection.dropBackground": "#5f1b1b",
        "panelSectionHeader.background": "#993333",
        // =====================================================================
        // Lists
        "list.activeSelectionBackground": "#993333cc",
        "list.activeSelectionForeground": "#ffffff",
        "list.focusBackground": "#993333cc",
        "list.focusForeground": "#b69f9f",
        "list.highlightForeground": "#ffffff",
        "list.hoverBackground": "#6d3434cc",
        "list.hoverForeground": "#b69f9f",
        "list.inactiveSelectionBackground": "#7e5f5fcc",
        "list.inactiveSelectionForeground": "#cca2a2",
        "list.invalidItemForeground": "#cc9900",
        // =====================================================================
        // Buttons
        "button.background": "#993333",
        "button.foreground": "#ff9999",
        "button.hoverBackground": "#803434",
        "button.secondaryBackground": "#993333",
        "button.secondaryForeground": "#ff9999",
        "button.secondaryHoverBackground": "#803434",
        // =====================================================================
        // Dropdowns
        "dropdown.background": "#352828",
        "dropdown.border": "#993333",
        "dropdown.foreground": "#b69f9f",
        "dropdown.listBackground": "#993333",
        // =====================================================================
        // Scroll Bar
        "scrollbar.shadow": "#15181b",
        "scrollbarSlider.activeBackground": "#cc333366",
        "scrollbarSlider.background": "#99333366",
        "scrollbarSlider.hoverBackground": "#80343466",
        // =====================================================================
        // Input Boxes
        "input.background": "#352828",
        "input.border": "#993333",
        "input.foreground": "#b69f9f",
        "input.placeholderForeground": "#6d3434",
        "inputOption.activeForeground": "#b69f9f",
        "inputOption.activeBorder": "#cc3333",
        "inputValidation.errorBackground": "#996633",
        "inputValidation.errorForeground": "#ffffff",
        "inputValidation.infoBackground": "#b86161",
        "inputValidation.infoForeground": "#ffffff",
        "inputValidation.warningBackground": "#cc9900",
        "inputValidation.warningForeground": "#ffffff",
        // =====================================================================
        // Tabs
        "tab.activeBackground": "#5f1b1b",
        "tab.activeForeground": "#b69f9f",
        "tab.border": "#993333",
        "tab.inactiveBackground": "#7e5f5f",
        "tab.inactiveForeground": "#cca2a2",
    },
    "tokenColors": [
        {
            "name": "Unmarked",
            "scope": [
                "source",
            ],
            "settings": {
                "foreground": "#00ff00"
            }
        },
        // =====================================================================
        // Text
        // =====================================================================
        {
            "name": "Default",
            "scope": [
                "entity.name",
                "meta.use.rust",
                "punctuation.accessor",
                "punctuation.brackets",
                "punctuation.comma",
                "punctuation.curlybrace",
                "punctuation.definition.typeparameters",
                "punctuation.definition.parameters",
                "punctuation.parenthesis",
                "punctuation.squarebracket",
                "punctuation.section",
                "punctuation.semi",
                "punctuation.separator",
                "punctuation.terminator",
                "source",
            ],
            "settings": {
                "foreground": "#b69f9f"
            }
        },
        // =====================================================================
        // Code
        // =====================================================================
        {
            "name": "Comment",
            "scope": [
                "comment",
                "punctuation.definition.comment",
            ],
            "settings": {
                "foreground": "#486e4b"
            }
        },
        {
            "name": "String",
            "scope": [
                "string",
                "punctuation.definition.string",
            ],
            "settings": {
                "foreground": "#6fad6e"
            }
        },
        {
            "name": "StringEscapeCharacter",
            "scope": [
                "constant.character.escape",
                "punctuation.definition.interpolation",
            ],
            "settings": {
                "foreground": "#aca374"
            }
        },
        {
            "name": "Preprocessor",
            "scope": [
                "meta.attribute",
                "meta.attribute keyword",
                "meta.attribute punctuation.brackets",
                "punctuation.definition.annotation",
                "storage.type.annotation",
            ],
            "settings": {
                "foreground": "#bea73c"
            }
        },
        {
            "name": "Keyword",
            "scope": [
                "keyword",
                "constant.language",
                "storage.modifier",
                "variable.language",
            ],
            "settings": {
                "foreground": "#979bd6"
            }
        },
        {
            "name": "Class",
            "scope": [
                "entity.name.type.class",
                "entity.name.type.kotlin",
                "entity.name.type.enum",
                "entity.name.type.struct",
                "entity.other.inherited-class",
                "storage.type",
                "variable.other.object",
            ],
            "settings": {
                "foreground": "#7694f5"
            }
        },
        {
            "name": "Interface",
            "scope": [
                "entity.name.type.interface",
            ],
            "settings": {
                "foreground": "#26b995"
            }
        },
        {
            "name": "Constant",
            "scope": [
                "variable.other.constant",
                "source.powershell support.constant.variable",
                "constant.other.enum",
            ],
            "settings": {
                "foreground": "#c589e7"
            }
        },
        {
            "name": "Field",
            "scope": [
                "entity.name.variable.field",
                "variable.other",
                "source.powershell support.variable",
            ],
            "settings": {
                "foreground": "#db9052"
            }
        },
        {
            "name": "Method",
            "scope": [
                "entity.name.function",
                "entity.name.variable.property",
                "variable.other.object.property",
                "source.powershell support.function"
            ],
            "settings": {
                "foreground": "#fc7676"
            }
        },
        {
            "name": "Parameter",
            "scope": [
                "variable.parameter",
                "entity.name.variable.parameter",
                "entity.name.type.type-parameter",
                "storage.type.generic",
                "source.java punctuation.bracket.angle"
            ],
            "settings": {
                "foreground": "#bea73c"
            }
        },
        {
            "name": "Number",
            "scope": [
                "constant.numeric",
            ],
            "settings": {
                "foreground": "#fa8484"
            }
        },
        {
            "name": "DocCommentTagName",
            "scope": [
                "punctuation.definition.tag",
                "keyword.other.documentation",
                "entity.name.tag",
            ],
            "settings": {
                "foreground": "#4e6350"
            }
        },
        {
            "name": "DocCommentAttributeName",
            "scope": [
                "entity.other.attribute-name",
            ],
            "settings": {
                "foreground": "#3c7063",
            }
        },
        {
            "name": "DocCommentText",
            "scope": [
                "comment.block.documentation",
                "comment.block.javadoc",
                "comment.line.documentation",
            ],
            "settings": {
                "foreground": "#667068",
            }
        },
        {
            "name": "DocCommentParameter",
            "scope": [
                "comment.block.documentation string.quoted.double",
                "comment.block.documentation punctuation.definition.string",
            ],
            "settings": {
                "foreground": "#726427"
            }
        },
        // =====================================================================
        // XML
        // =====================================================================
        {
            "name": "XmlPunctuation",
            "scope": [
                "text.xml punctuation.definition.tag",
                "text.xml meta.tag",
            ],
            "settings": {
                "foreground": "#aca374"
            }
        },
        {
            "name": "XmlTagName",
            "scope": [
                "text.xml entity.name.tag",
            ],
            "settings": {
                "foreground": "#fc7676"
            }
        },
        {
            "name": "XmlAttributeName",
            "scope": [
                "text.xml entity.other.attribute-name",
            ],
            "settings": {
                "foreground": "#7694f5"
            }
        },
        // =====================================================================
        // YAML/JSON
        // =====================================================================
        {
            "name": "YamlPunctuation",
            "scope": [
                "source.yaml punctuation.definition",
                "source.json punctuation.definition",
                "source.yaml punctuation.separator",
                "source.json punctuation.separator",
                "source.yaml entity.other",
            ],
            "settings": {
                "foreground": "#b69f9f"
            }
        },
        {
            "name": "YamlEntityName",
            "scope": [
                "source.yaml entity.name.tag",
                "source.json support.type",
            ],
            "settings": {
                "foreground": "#7694f5"
            }
        },
        {
            "name": "YamlConstant",
            "scope": [
                "source.yaml constant.language",
                "source.json constant.language",
            ],
            "settings": {
                "foreground": "#c589e7"
            }
        },
        // =====================================================================
        // Markdown
        // =====================================================================
        {
            "name": "Markdown Headers 1",
            "scope": [
                "heading.1 entity.name.section",
                "punctuation.definition.heading",
            ],
            "settings": {
                "foreground": "#7694f5",
            }
        },
        {
            "name": "Markdown Headers 2",
            "scope": [
                "heading.2 entity.name.section",
            ],
            "settings": {
                "foreground": "#c589e7"
            }
        },
        {
            "name": "Markdown Headers 3",
            "scope": [
                "heading.3 entity.name.section",
            ],
            "settings": {
                "foreground": "#26b995"
            }
        },
        {
            "name": "Other Markdown Headers",
            "scope": [
                "entity.name.section",
            ],
            "settings": {
                "foreground": "#4f7696"
            }
        },
        {
            "name": "Markdown Reference Links",
            "scope": [
                "constant.other.reference.link",
            ],
            "settings": {
                "foreground": "#979bd6"
            }
        },
        {
            "name": "Markdown Links",
            "scope": [
                "markup.underline.link",
            ],
            "settings": {
                "foreground": "#26b995"
            }
        },
        {
            "name": "Markdown Punctuation",
            "scope": [
                "fenced_code.block.language",
                "markup.inline",
                "string.other.link",
                "text.html.markdown punctuation.definition",
                "text.html.markdown punctuation.definition.string",
                "text.html.markdown meta.separator",
            ],
            "settings": {
                "foreground": "#aca374"
            }
        },
        // =====================================================================
        // Properties
        // =====================================================================
        {
            "name": "Properties Name",
            "scope": [
                "source.ini keyword",
            ],
            "settings": {
                "foreground": "#7694f5"
            }
        },
        {
            "name": "Section Title",
            "scope": [
                "source.ini entity.name.section",
            ],
            "settings": {
                "foreground": "#26b995"
            }
        },
        {
            "name": "Properties Value",
            "scope": [
                "source.ini",
            ],
            "settings": {
                "foreground": "#c589e7"
            }
        },
        {
            "name": "Punctuation",
            "scope": [
                "source.ini punctuation",
                "source.ini punctuation.separator",
            ],
            "settings": {
                "foreground": "#aca374"
            }
        },
        // =====================================================================
        // TOML
        // =====================================================================
        {
            "name": "Properties Name",
            "scope": [
                "source.toml keyword",
            ],
            "settings": {
                "foreground": "#7694f5"
            }
        },
        {
            "name": "Section Title",
            "scope": [
                "source.toml entity.other.attribute-name",
            ],
            "settings": {
                "foreground": "#26b995"
            }
        },
        {
            "name": "Properties Value",
            "scope": [
                "source.toml",
            ],
            "settings": {
                "foreground": "#c589e7"
            }
        },
        {
            "name": "Punctuation",
            "scope": [
                "source.toml punctuation",
                "source.toml punctuation.separator",
            ],
            "settings": {
                "foreground": "#aca374"
            }
        },
    ],
    "semanticTokenColors": {
        // Default Text
        "local": "#b69f9f",
        "namespace": "#b69f9f",
        "operator": "#b69f9f",
        "punctuation": "#b69f9f",
        "variable": "#b69f9f",
        // Comment
        "comment": "#486e4b",
        "comment.documentation": "#6b776d",
        // String
        "string": "#6fad6e",
        "regexp": "#79d6c7",
        // String Excapes
        "stringEscapeCharacter": "#aca374",
        // Keywords
        "keyword": "#979bd6",
        "controlKeyword": "#979bd6",
        "plainKeyword": "#979bd6",
        // Concrete types
        "class": "#7694f5",
        "enum": "#7694f5",
        "struct": "#7694f5",
        // Interface
        "interface": "#26b995",
        "delegate": "#26b995",
        // Constant values
        "constant": "#c589e7",
        "enumMember": "#c589e7",
        "variable.static.readonly": "#c589e7",
        "property.static.readonly": "#c589e7",
        "field.static": "#c589e7",
        // Fields
        "field": "#db9052",
        "property:java": "#db9052",
        // Methods
        "method": "#fc7676",
        "property": "#fc7676",
        // Parameters and Arguments
        "parameter": "#bea73c",
        "typeParameter": "#bea73c",
        // Numbers
        "number": "#fa8484",
        // Doc Comment Attribute
        "xmlDocCommentAttributeName": "#4f7696",
        // Doc Comment Tokens
        "xmlDocCommentName": "#8b7364",
        "xmlDocCommentAttributeQuotes": "#4e6350",
        "xmlDocCommentDelimiter": "#4e6350",
        // Doc Comment Text
        "xmlDocCommentText": "#6b776d",
        // Deprecated values
        "*.deprecated": {
            "strikethrough": true
        }
    }
}